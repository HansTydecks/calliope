{
  "name": "vitepress-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build-and-deploy": "npm run docs:build && git add -A && git diff --cached --quiet && echo Nothing to commit, deployment ready. || (git commit -m \"chore: update docs site\" && git push)"
  },
  "devDependencies": {
    "vue": "^3.4.0",
    "vitepress": "^1.5.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
